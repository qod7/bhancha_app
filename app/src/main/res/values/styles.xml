<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="android:Theme.Holo.Light">
        <!-- Customize your theme here. -->
        <item name="android:titleTextStyle">@style/MyActionBarTitleText</item>
        <!--item name="android:textColor">#0000ff</item> This changed nav drawer text color -->
    </style>
    <!-- ActionBar title text -->
    <style name="MyActionBarTitleText"
        parent="android:TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:paddingLeft">200dp</item>
        <item name="android:textColorPrimary">#ff0000</item>
        <item name="android:textColor">#00ff00</item>
    </style>

    <style name="AboutBhanchhaActivityTheme" parent="android:Theme.Holo.Light.NoActionBar" />
    <style name="LoginActivityTheme" parent="android:Theme.Holo.Light.NoActionBar" />

    <style name="OverlayActionBarTheme" parent="@android:Theme.Holo.Light">
        <item name="android:windowActionBarOverlay">true</item>
    </style>

    <color name="drawer_bg_color">#FDDD</color>

    <color name="login_views_bg">#cfff</color>

    <color name="app_bg_color">#eeeeee</color>

    <color name="blaak">#000</color>

</resources>
